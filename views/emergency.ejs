<script>
    const sliderInvis = document.getElementById('slider-line-invis')
    const slider2 = document.getElementById('slider2')
    const circle = document.getElementById('circle')
    const money = document.getElementById('money')

    sliderInvis.addEventListener('click', (event) => {
        getDivPosition(event)
    })
    
    function getDivPosition(event) {
        var rect = sliderInvis.getBoundingClientRect();
        var xPosition = event.clientX - rect.left;
        // console.log(rect.left, event.clientX)
        var percentage = xPosition/rect.width * 100
        console.log(xPosition/rect.width * 100)
        slider2.style.width = `${percentage}%`
        circle.style.left = `${percentage - 5}%`
        const moneyNum = money.innerText

        money.innerText = Math.round((percentage*20000/100)/50) * 50
        
    }
</script>
